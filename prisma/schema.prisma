generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Categoria {
  categoria String @id
  icon      String

  Despesa Despesa[]

  @@map("categorias")
}

model Despesa {
  id        String  @id @default(uuid())
  descricao String
  categoria String
  valor     Decimal

  id_categoria Categoria @relation(fields: [categoria], references: [categoria])

  @@map("despesas")
}
